CREATE TABLE Z_ESTATUS_RECALL(
IDRECALL NUMBER NOT NULL,
FECHA DATE,
TIPO VARCHAR(50),
DESCRIPCION VARCHAR(50),
INICIO DATE,
FIN DATE, 
SKUS INT,
PZS_RECALL INT,
PZS_ENVIADAS INT, 
PZS_PROCESADAS INT,
PZS_INGRESADAS INT,
ESTATUS VARCHAR(50), 
TURNO VARCHAR(12),
LOCACION VARCHAR(20),
RGA VARCHAR(50),
COMENTARIOS VARCHAR(50),
VENDOR INT, 
TIPO_VENDOR VARCHAR(50),
USUARIO VARCHAR(20),
PRIMARY KEY (IDRECALL)
)
